{
   "expo": {
      "name": "ts-app",
      "slug": "ts-app",
      "privacy": "public",
      "sdkVersion": "32.0.0",
      "platforms": ["ios", "android"],
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "updates": {
         "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true
      },
      "ignoreNodeModulesValidation": true,
      "packagerOpts": {
         "config": "rn-cli.config.js",
         "projectRoots": "",
         "sourceExts": ["ts", "tsx"],
         "transformer": "node_mdules/react-native-typescript-transformer/index.js"
      }
   }
}
